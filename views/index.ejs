<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/stylesheets/style.css?ver=12" />
    <%- include('./vendors.ejs') %>

    <title>main</title>
  </head>

  <body>
    <div class="container-fluid">
      <div class="row">
        <div class="col-xxl-6 col-xl-12 border shadow pb-3">
          <%- include('./target-search.ejs') %>
        </div>
        <div class="col-xxl-6 col-xl-12 border shadow">
          <%- include('./send-email.ejs') %>
        </div>
      </div>
    </div>
    <!-- Toast -->
    <div
      id="toast"
      class="toast align-items-center text-bg-primary border-0 start-50 translate-middle"
      role="alert"
      aria-live="assertive"
      aria-atomic="true"
    >
      <div class="d-flex">
        <div class="toast-body"></div>
        <button
          type="button"
          class="btn-close btn-close-white me-2 m-auto"
          data-bs-dismiss="toast"
          aria-label="Close"
        ></button>
      </div>
    </div>

    <!-- <script type="text/javascript" src="/js/index.js?ver=3.9"></script> -->
    <!-- <script type="text/javascript" src="/js/indexFunctions.js?ver=3.8"></script> -->
    <script type="module" src="/js/main.js"></script>
    <script type="text/javascript" src="/js/server-state.js?ver=1.0"></script>
  </body>
</html>
